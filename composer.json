{
    "name": "playerom/translations-scan",
    "description": "Scan Laravel project for obtain all texts which will need translations.",
    "keywords": ["laravel", "translations", "scan", "locale", "languages"],
    "license": "GNU v3.0",
    "authors": [
        {
            "name": "PlayeRom",
            "email": "roman.ludwicki@playerom.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "~5.8|^6.0",
        "illuminate/filesystem": "~5.8|^6.0",
        "illuminate/support": "~5.8|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.3"
    },
	"autoload": {
        "psr-4": {
            "PlayeRom\\TranslationsScan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\TranslationsScan\\Tests\\": "tests/"
        }
    },
	"extra": {
        "laravel": {
            "providers": [
                "PlayeRom\\TranslationsScan\\TranslScanServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
